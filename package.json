{
  "name": "vue-todo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 5300",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "server": "json-server --watch db.json --port 3001",
    "server:dev": "npm-run-all2 --parallel dev server",
    "server:hello": "node server.cjs",
    "server:static": "http-server dist -p 3001"
  },
  "dependencies": {
    "@primeuix/themes": "^1.0.0",
    "@primevue/auto-import-resolver": "4.3.6",
    "@primevue/forms": "^4.3.6",
    "axios": "^1.7.9",
    "pinia": "^3.0.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.6",
    "tailwindcss-primeui": "^0.5.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "zod": "3.23"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.19",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "1.1.31",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.20.1",
    "eslint-plugin-vue": "^9.32.0",
    "http-server": "^14.1.1",
    "jsdom": "^25.0.1",
    "json-server": "^0.17.4",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.7",
    "vue-tsc": "^2.2.4"
  }
}